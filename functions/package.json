{
  "name": "functions",
  "scripts": {
    "lint": "./node_modules/.bin/tslint -p tslint.json",
    "build": "./node_modules/.bin/tsc",
    "serve": "npm run build && ./node_modules/.bin/firebase-tools/bin/firebase serve --only functions",
    "shell": "npm run build && ./node_modules/.bin/firebase-tools/bin/firebase experimental:functions:shell",
    "start": "npm run shell",
    "deploy": "./node_modules/.bin/firebase-tools/bin/firebase deploy --only functions --token $FIREBASE_TOKEN",
    "logs": "./node_modules/.bin/firebase-tools/bin/firebase functions:log"
  },
  "main": "lib/index.js",
  "dependencies": {
    "firebase-admin": "~5.8.1",
    "firebase-functions": "^0.8.1",
    "firebase-tools": "^3.17.4",
    "twitter-node-client": "0.0.6"
  },
  "devDependencies": {
    "tslint": "^5.8.0",
    "typescript": "^2.5.3"
  },
  "private": true
}
